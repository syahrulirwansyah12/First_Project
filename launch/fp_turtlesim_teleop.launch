<launch>
    <arg name="key_timeout" default="0.5"/>
    <arg name="repeat_rate" default="10.0"/>
    <remap from="/turtle1/cmd_vel" to="/cmd_vel" />
    <node pkg="turtlesim" type="turtlesim_node" name="turtlesim_node"  output="screen"/>
    <node pkg="first_project" type="fp_teleop_key.py" name="fp_teleop_keyboard"  output="screen"> 
        <param name="key_timeout" value="$(arg key_timeout)" />
        <param name="repeat_rate" value="$(arg repeat_rate)" />
    </node>
</launch>